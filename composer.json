{
    "name": "queen-tech-solution/laravel-paymob",
    "description": "Laravel PayMob online payment gateway package based on baklysystems",
    "license": "MIT",
    "keywords": [
        "egypt"
    ],
    "homepage": "https://github.com/MohamedEmadQueenTechSolution/Paymob-Laravel",
    "authors": [
        {
            "name": "Mohamed Emad",
            "email": "omda@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "BaklySystems\\PayMob\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BaklySystems\\PayMob\\PayMobServiceProvider"
            ],
            "aliases": {
                "PayMob": "BaklySystems\\PayMob\\Facades\\PayMob"
            }
        }
    }
}
